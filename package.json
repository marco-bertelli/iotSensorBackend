{
	"name": "simon-e-buildings-notifications",
	"version": "1.6.1",
	"main": "build",
	"private": true,
	"engines": {
		"node": "~14.0.0"
	},
	"scripts": {
		"start": "npm run build && node .",
		"test": "mocha --compilers js:babel-core/register --recursive",
		"test:unit": "npm test \"src/**/!(index).test.js\"",
		"test:integration": "npm test \"src/**/index.test.js\"",
		"coverage": "npm test -- --coverage",
		"dev": "npx nodemon ./src/index.ts",
		"build": "npx tsc",
		"debug": "nodemon --inspect -i \"*.test.js\" .",
		"dev-inspect": "nodemon --inspect -i \"*.test.js\" .",
		"prod": "cross-env NODE_ENV=production nodemon -i \"*.test.js\" -r dotenv-safe/config .",
		"lint": "eslint src",
		"docs": "apidoc --parse-languages default=./documentation/default.js -i src -o docs && apidoc-markdown2 -p docs -o README.md",
		"precommit": "lint-staged",
		"getversion": "echo $npm_package_version",
		"release": "standard-version"
	},
	"lint-staged": {
		"src/**/*.js": [
			"eslint --fix",
			"git add"
		]
	},
	"devDependencies": {
		"chai": "^4.1.2",
		"chai-as-promised": "^7.1.1",
		"cross-env": "^3.1.3",
		"eslint": "^4.19.0",
		"husky": "^0.14.3",
		"jest-cli": "^18.1.0",
		"lint-staged": "^7.0.0",
		"mocha": "^5.1.1",
		"nock": "^9.0.2",
		"nodemon": "^1.19.4",
		"sinon": "^1.17.5",
		"supertest": "^2.0.1"
	},
	"dependencies": {
		"@azure/msal-node": "^1.7.0",
		"@types/bluebird": "^3.5.36",
		"@types/bluebird-global": "^3.5.13",
		"@types/compression": "^1.7.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/lodash": "^4.14.181",
		"@types/node": "^17.0.23",
		"@types/node-schedule": "^1.3.2",
		"@types/winston": "^2.4.4",
		"amqplib": "^0.8.0",
		"apidoc": "^0.16.1",
		"apidoc-markdown2": "^0.3.6",
		"babel-core": "^6.17.0",
		"babel-eslint": "^7.0.0",
		"babel-jest": "^18.0.0",
		"babel-plugin-transform-runtime": "^6.15.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-stage-1": "^6.16.0",
		"babel-runtime": "^6.11.6",
		"bluebird": "^3.4.0",
		"body-parser": "^1.15.1",
		"bodymen": "^1.0.1",
		"compression": "^1.6.2",
		"cors": "^2.7.1",
		"date-fns": "^2.28.0",
		"date-fns-tz": "^1.3.3",
		"dotenv-safe": "^4.0.3",
		"express": "^4.13.4",
		"express-force-ssl": "^0.3.2",
		"express-winston": "^4.0.5",
		"firebase-admin": "^10.0.2",
		"influx": "^5.0.7",
		"influx-connection-url-parser": "^1.0.1",
		"influxdb-nodejs": "^2.7.7",
		"json-rules-engine": "^2.3.0",
		"jsonschema": "^1.2.4",
		"jsonwebtoken": "^7.1.9",
		"lodash": "^4.13.1",
		"moment": "^2.19.1",
		"moment-business-days": "^1.2.0",
		"mongoose": "6.1.2",
		"node-cache": "^4.2.0",
		"node-pushnotifications": "1.0.18",
		"node-schedule": "^1.3.2",
		"proper-url-join": "^2.1.1",
		"querymen": "^2.0.0",
		"rand-token": "^0.3.0",
		"request": "^2.74.0",
		"request-promise": "^4.1.1",
		"s3fs": "^2.5.0",
		"sendgrid": "^4.0.2",
		"shortid32": "^0.1.1",
		"standard-version": "^9.5.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3",
		"util": "^0.10.3",
		"uuid": "^3.1.0",
		"winston": "^3.3.3"
	}
}
